{
  "name": "cannaai-pro-hybrid",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd NewUI/cannaai-pro && npm run dev",
    "dev:backend": "nodemon --exec \"tsx server.ts\" --watch server.ts --watch src --ext ts,tsx,js,jsx",
    "dev:win": "concurrently \"npm run dev:backend:win\" \"npm run dev:frontend\"",
    "dev:backend:win": "nodemon --exec \"tsx server.ts\" --watch server.ts --watch src --ext ts,tsx,js,jsx",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd NewUI/cannaai-pro && npm run build",
    "build:backend": "next build",
    "build:static": "cross-env BUILD_MODE=static next build",
    "build:netlify": "npm run build:static",
    "build:frontend-only": "cd NewUI/cannaai-pro && npm run build",
    "preview": "cd NewUI/cannaai-pro && npm run preview",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd NewUI/cannaai-pro && npm run preview --port 5173 --host 0.0.0.0",
    "start:backend": "NODE_ENV=production tsx server.ts",
    "start:win": "concurrently \"npm run start:backend:win\" \"npm run start:frontend\"",
    "start:backend:win": "set NODE_ENV=production && tsx server.ts",
    "lint": "npm run lint:frontend && npm run lint:backend",
    "lint:frontend": "cd NewUI/cannaai-pro && npm run lint || echo 'No lint script found in New UI'",
    "lint:backend": "next lint",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup": "npm install && cd NewUI/cannaai-pro && npm install",
    "setup:backend": "npm install && npm run db:generate",
    "setup:frontend": "cd NewUI/cannaai-pro && npm install",
    "install:all": "npm install && cd NewUI/cannaai-pro && npm install",
    "check:ports": "tsx scripts/check-ports.ts",
    "health": "tsx scripts/health-check.ts",
    "clean": "npm run clean:frontend && npm run clean:backend",
    "clean:frontend": "cd NewUI/cannaai-pro && rm -rf node_modules && npm install",
    "clean:backend": "rm -rf node_modules && npm install",
    "start:both": "tsx scripts/start-both.ts",
    "start:dev": "tsx scripts/start-both.ts --dev",
    "start:prod": "tsx scripts/start-both.ts --prod",
    "deploy:prod": "tsx scripts/deploy-production.ts",
    "deploy:build": "tsx scripts/deploy-production.ts --build-only",
    "deploy:check": "tsx scripts/deploy-production.ts --check-only"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
